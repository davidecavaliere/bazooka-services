{
  "name": "@bazooka/users",
  "version": "1.10.6",
  "private": true,
  "description": "run your api code everywhere",
  "main": "index.js",
  "typings": "index.d.ts",
  "module": "index.js",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "clean": "trash lib",
    "prebuild": "run-s clean",
    "build": "tsc -p tsconfig.json",
    "build:watch": "nodemon -w src --exec 'yarn build'",
    "test": "jest --coverage",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "test:lint": "tslint --project . ",
    "watch": "run-s test:watch",
    "cov": "run-s build test:unit cov:html && opn coverage/index.html",
    "cov:html": "nyc report --reporter=html",
    "cov:send": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "cov:check": "nyc report && nyc check-coverage --lines 100 --functions 100 --branches 100",
    "doc": "run-s doc:html && opn build/docs/index.html",
    "doc:html": "typedoc src/ --target ES6 --mode file --out build/docs",
    "doc:json": "typedoc src/ --target ES6 --mode file --json build/docs/typedoc.json",
    "doc:publish": "gh-pages -m \"[ci skip] Updates\" -d build/docs",
    "prepare-release": "run-s all version doc:publish",
    "sls": "sls",
    "deploy": "ls -al",
    "wait": "sleep 10",
    "watch:build-output": "nodemon -w lib --exec \"ls -al lib\"",
    "watch:sls:offline": "run-p build:watch watch:build-output",
    "sls:offline": "AUTHORIZER='{\"principalId\": \"123\"}' SLS_DEBUG=* DEBUG=microgamma:* nodemon -w lib --exec \"sls offline start --host 0.0.0.0 --noAuth\"",
    "sls:deploy:dev": "yarn build && DEBUG=microgamma:* yarn sls deploy --stage dev",
    "deploy:dev": "DEBUG=microgamma:* yarn sls deploy --stage dev"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(spec))\\.ts?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "reporters": [
      "default"
    ]
  },
  "dependencies": {
    "@microgamma/apigator": "^1.12.3",
    "@microgamma/datagator": "^1.12.3",
    "@microgamma/dynamodb": "^1.12.3",
    "@microgamma/digator": "^1.12.3",
    "@microgamma/loggator": "^1.12.3",
    "jsonwebtoken": "^8.4.0"
  },
  "devDependencies": {
    "serverless": "^1.30.1",
    "serverless-dynamodb-local": "^0.2.35",
    "serverless-domain-manager": "^2.6.11",
    "serverless-offline": "^3.25.9"
  }
}
